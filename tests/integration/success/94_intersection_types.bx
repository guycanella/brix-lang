// Intersection types (Point & Label)

struct Point {
    x: int,
    y: int
}

struct Label {
    name: string
}

// For now, use inline Intersection type instead of alias
// Future: support NamedPoint{ ... } construction

var p1: Point = Point{ x: 10, y: 20 }
